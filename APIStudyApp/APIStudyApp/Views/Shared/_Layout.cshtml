                                @Html.ActionLink("JSON Övningar", "Index", "AjaxJson", new { area = "" }, htmlAttributes: new { @class = "dropdown-toggle", data_toggle = "dropdown" })
                                <ul class="dropdown-menu">
                                    <li> @Html.ActionLink("Övning 1", "ComingSoon", "Home", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 2", "ComingSoon", "Home", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 3", "ComingSoon", "Home", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 4", "ComingSoon", "Home", new { area = "" }, null)</li>
