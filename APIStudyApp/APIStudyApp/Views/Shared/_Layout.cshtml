                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Hem", "Index", "Home", new { area = "" }, null)</li>
                    <li>

                        <a href="Facit" class="dropdown-toggle" data-toggle="dropdown">Facit <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li class="dropdown-submenu">

                                @Html.ActionLink("Ajax med Texter", "AjaxTextFacit", "Home", new { area = "" }, htmlAttributes: new { @class = "dropdown-toggle", data_toggle = "dropdown" })
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("AjaxTextFacit","Home")#link1">Lösning 1</a></li>
                                    <li><a href="@Url.Action("AjaxTextFacit","Home")#link2">Lösning 2</a></li>
