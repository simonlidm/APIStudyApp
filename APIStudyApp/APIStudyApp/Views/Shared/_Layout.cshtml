                                    <li><a href="@Url.Action("AjaxTextFacit","Home")#link6">Lösning 6</a></li>
                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                @Html.ActionLink("Ajax med JSON", "AjaxJsonFacit", "Home", new { area = "" }, htmlAttributes: new { @class = "dropdown-toggle", data_toggle = "dropdown" })
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link1">Lösning 1</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link2">Lösning 2</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link3">Lösning 2 - Utmaning</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link4">Lösning 3</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link5">Lösning 4</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link6">Lösning 5</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link7">Lösning 6</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link8">Lösning 7</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link9">Lösning 8</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link10">Lösning 9</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link11">Lösning 10</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link12">Lösning 11</a></li>
                                    <li><a href="@Url.Action("AjaxJsonFacit","Home")#link13">Lösning 12</a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>

                    <li>
                        <a href="AjaxText" class="dropdown-toggle" data-toggle="dropdown">Övningar <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li class="dropdown-submenu">

                                @Html.ActionLink("Text Övningar", "Index", "AjaxText", new { area = "" }, htmlAttributes: new { @class = "dropdown-toggle", data_toggle = "dropdown" })
                                <ul class="dropdown-menu">
                                    <li> @Html.ActionLink("Övning 1", "Example_1", "AjaxText", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 2", "Example_2", "AjaxText", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 3", "Example_3", "AjaxText", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 4", "Example_4", "AjaxText", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 5", "Example_5", "AjaxText", new { area = "" }, null)</li>
                                    <li> @Html.ActionLink("Övning 6", "Example_6", "AjaxText", new { area = "" }, null)</li>
                                </ul>
                            </li>
